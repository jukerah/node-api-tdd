{
  "name": "node-api-tdd",
  "version": "1.0.0",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "swagger-bundle": "swagger-cli bundle --outfile src/docs/bundled-swagger.json src/docs/main-swagger.json",
    "dev": "concurrently \"yarn swagger-bundle\" \"ts-node-dev --watch --transpile-only src/server.ts\"",
    "dev:in-memory": "concurrently \"yarn swagger-bundle\" \"ts-node-dev --watch --transpile-only src/__mocks__/server.ts\"",
    "test": "jest --config jest.config.ts",
    "test:coverage": "jest --coverage --config jest.config.ts",
    "test:e2e": "jest --config jest-e2e.config.ts",
    "build": "tsc",
    "lint": "eslint src/"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.47.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.7.0",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.1",
    "nodemailer": "^6.9.4",
    "nodemailer-mock": "^2.0.1",
    "prisma": "^5.1.0",
    "swagger-cli": "^4.0.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@prisma/client": "^5.1.0",
    "@swc/core": "^1.3.59",
    "@swc/jest": "^0.2.26",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.2.1",
    "@types/nodemailer": "^6.4.9",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "concurrently": "^8.2.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
